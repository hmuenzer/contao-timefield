{
    "name":"hmuenzer/timefield",
    "version":"1.0.0-alpha",
    "description":"Frontend text field with timepicker for Contao Open Source CMS (uses jQuery)",
    "keywords":["contao", "Time Field", "Timepicker", "jQuery"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name": "Holger MÃ¼nzer",
            "homepage": "https://github.com/hmuenzer",
            "email": "holger.muenzer@gmail.com",
            "role": "Developer"
        }
    ],
    "require":{
        "php": "^7.2 || ^8.0",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4 || ^5.1",
        "symfony/dependency-injection": "^4.4 || ^5.1",
        "symfony/http-kernel": "^4.4 || ^5.1"
    },
    "autoload": {
        "psr-4": {
            "Hmuenzer\\TimeField\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/"
        ]
    },
    "extra":{
        "contao": {
            "contao-manager-plugin": "Hmuenzer\\TimeField\\ContaoManager\\Plugin"
        }
    }
}
